# Danish/Swedish character map.
#
# $Id: string.chr,v 1.2 1998-01-26 10:36:49 adam Exp $

# Define the basic value-set. *Beware* of changing this without re-indexing
# your databases.

lowercase {0-9}{a-y}zזהרצו
uppercase {0-9}{A-Y}Zִֶ״ײֵ

# Breaking characters

space {\001-\040}!"#$%&'\()*+,-./:;<=>?@\[\\]^_`\{|}~

# Characters to be considered equivalent for searching purposes.

# equivalent זה(ae)
# equivalent רצ(oe)
# equivalent ו(aa)
# equivalent u

# Supplemental mappings

map (&aelig;)      ז
map (&oslash;)     ר
map (&aring;)      ו
map (&Aelig;)      ֶ
map (&Oslash;)     ״
map (&Aring;)      ֵ

map יֹ		e
map ב		a
map ף		o
map ם		i

map (Aa)	(AA)
